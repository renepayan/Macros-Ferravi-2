CREATE TABLE IF NOT EXISTS tbl_Usuario(id INT PRIMARY KEY NOT NULL,usuario VARCHAR(100) UNIQUE KEY NOT NULL,password VARCHAR(100) NOT NULL,activo DATETIME default NULL)
CREATE TABLE IF NOT EXISTS tbl_Usuario_Saldos(id INT PRIMARY KEY NOT NULL,usuario VARCHAR(100) UNIQUE KEY NOT NULL,password VARCHAR(100) NOT NULL,activo DATETIME default NULL)
CREATE TABLE IF NOT EXISTS tbl_Campana(id INT PRIMARY KEY NOT NULL,id_usuario_saldo int,nombre VARCHAR(100) UNIQUE KEY NOT NULL)
CREATE TABLE IF NOT EXISTS tbl_Registro(id INT PRIMARY KEY NOT NULL,id_campana INT NOT NULL,cuenta varchar(100),estatus_cuenta varchar(100), estatus_recarga varchar(100),saldo_total varchar(100),total_pagos varchar(100),zona varchar(100),recargas_realizadas varchar(100),recargas_faltantes varchar(100),tarjeta1 varchar(100),equipo1 varchar(100),estatus1 varchar(100),tarjeta2 varchar(100),equipo2 varchar(100),estatus2 varchar(100),tarjeta3 varchar(100),equipo3 varchar(100),estatus3 varchar(100),tarjeta4 varchar(100),equipo4 varchar(100),estatus4 varchar(100));
